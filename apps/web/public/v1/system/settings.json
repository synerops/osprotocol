{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://osprotocol.dev/v1/system/settings.json",
  "title": "Settings Entry Schema",
  "description": "Platform settings entry (key-value with description)",
  "$ref": "#/definitions/SettingsEntry%3Cunknown%3E",
  "definitions": {
    "SettingsEntry<unknown>": {
      "type": "object",
      "properties": {
        "key": {
          "type": "string",
          "description": "Setting key"
        },
        "value": {
          "description": "Setting value"
        },
        "description": {
          "type": "string",
          "description": "Human-readable description"
        },
        "readOnly": {
          "type": "boolean",
          "description": "Whether this setting is read-only (cannot be changed at runtime)"
        },
        "metadata": {
          "$ref": "#/definitions/Record%3Cstring%2Cunknown%3E",
          "description": "Extensible metadata for provider-specific data"
        }
      },
      "required": [
        "key",
        "value"
      ],
      "additionalProperties": false,
      "description": "A system setting entry"
    },
    "Record<string,unknown>": {
      "type": "object",
      "additionalProperties": {}
    }
  }
}
