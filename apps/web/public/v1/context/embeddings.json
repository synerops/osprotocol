{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://osprotocol.dev/v1/context/embeddings.json",
  "title": "Embedding Entry Schema",
  "description": "Vector embedding entry with content, score, and metadata",
  "$ref": "#/definitions/EmbeddingEntry%3CRecord%3Cstring%2Cunknown%3E%3E",
  "definitions": {
    "EmbeddingEntry<Record<string,unknown>>": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier"
        },
        "content": {
          "type": "string",
          "description": "Original content that was embedded"
        },
        "score": {
          "type": "number",
          "description": "Similarity score (0-1, present only in search results)"
        },
        "metadata": {
          "$ref": "#/definitions/Record%3Cstring%2Cunknown%3E",
          "description": "Extensible metadata for filtering and provider-specific data"
        }
      },
      "required": [
        "id",
        "content"
      ],
      "additionalProperties": false,
      "description": "A vector embedding entry"
    },
    "Record<string,unknown>": {
      "type": "object",
      "additionalProperties": {}
    }
  }
}
