{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://osprotocol.dev/v1/context/kv.json",
  "title": "Key-Value Entry Schema",
  "description": "Key-value persistence entry for agent state",
  "$ref": "#/definitions/KvEntry%3Cunknown%3E",
  "definitions": {
    "KvEntry<unknown>": {
      "type": "object",
      "properties": {
        "key": {
          "type": "string",
          "description": "Entry key"
        },
        "value": {
          "description": "Entry value"
        },
        "metadata": {
          "$ref": "#/definitions/Record%3Cstring%2Cunknown%3E",
          "description": "Extensible metadata for provider-specific data"
        }
      },
      "required": [
        "key",
        "value"
      ],
      "additionalProperties": false,
      "description": "A key-value entry"
    },
    "Record<string,unknown>": {
      "type": "object",
      "additionalProperties": {}
    }
  }
}
