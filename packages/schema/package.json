{
  "name": "@osprotocol/schema",
  "version": "0.1.0",
  "description": "TypeScript type definitions for the Agentic OS Protocol",
  "license": "MIT",
  "type": "module",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": "./index.ts",
    "./workflows": "./workflows/index.ts",
    "./runs": "./runs/index.ts",
    "./agent": "./agent.ts",
    "./skill": "./skill.ts",
    "./system/data": "./system/data/index.ts"
  },
  "files": [
    "**/*.ts",
    "!**/*.test.ts",
    "!**/*.spec.ts",
    "!scripts/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/synerops/osprotocol.git",
    "directory": "packages/schema"
  },
  "scripts": {
    "build": "bun run scripts/generate-schemas.ts",
    "typecheck": "bunx tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "ts-json-schema-generator": "^2.5.0",
    "typescript": "^5.9.3"
  }
}
