{
  "name": "@osprotocol/schema",
  "version": "0.1.0",
  "description": "TypeScript type definitions for the Agentic OS Protocol",
  "license": "MIT",
  "type": "module",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": "./index.ts",
    "./workflows": "./workflows/index.ts",
    "./runs": "./runs/index.ts",
    "./system/env": "./system/env.ts",
    "./system/fs": "./system/fs.ts",
    "./system/sandbox": "./system/sandbox.ts",
    "./system/settings": "./system/settings.ts",
    "./system/preferences": "./system/preferences.ts",
    "./system/registry": "./system/registry.ts",
    "./system/mcp-client": "./system/mcp-client.ts",
    "./system/installer": "./system/installer.ts",
    "./context/system": "./context/system.ts",
    "./context/embeddings": "./context/embeddings.ts",
    "./context/kv": "./context/kv.ts",
    "./actions/system": "./actions/system.ts",
    "./actions/tools": "./actions/tools.ts",
    "./actions/mcp-servers": "./actions/mcp-servers.ts",
    "./checks/rules": "./checks/rules.ts",
    "./checks/judge": "./checks/judge.ts",
    "./checks/audit": "./checks/audit.ts",
    "./checks/screenshot": "./checks/screenshot.ts",
    "./apps/schema": "./apps/schema.ts"
  },
  "files": [
    "**/*.ts",
    "!**/*.test.ts",
    "!**/*.spec.ts",
    "!scripts/**"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/synerops/osprotocol.git",
    "directory": "packages/schema"
  },
  "scripts": {
    "build": "bun run scripts/generate-schemas.ts",
    "typecheck": "bunx tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "ts-json-schema-generator": "^2.5.0",
    "typescript": "^5.9.3"
  }
}
